# Supabase Configuration
VITE_SUPABASE_URL=your_project_url_here
VITE_SUPABASE_ANON_KEY=your_anon_key_here

# AI API Keys
VITE_ANTHROPIC_API_KEY=your_anthropic_key_here
VITE_GOOGLE_AI_API_KEY=your_google_ai_key_here

# Email Service (Resend) - For approval/rejection emails
VITE_RESEND_API_KEY=your_resend_key_here

# Slack Notifications - Required for frontend notifications (booking confirmations, delivery alerts)
# Get your webhook URL from: https://api.slack.com/apps > Your App > Incoming Webhooks
VITE_SLACK_WEBHOOK_URL=your_slack_webhook_url_here

# Note: Database triggers also use Slack (configured in Supabase)
# See NOTIFICATION_SERVICES_SETUP.md for complete Slack setup

# Feature Flags
# Enable tile-based admin dashboard (false = legacy tab-based UI, true = new tile UI)
VITE_ENABLE_TILE_ADMIN=false
